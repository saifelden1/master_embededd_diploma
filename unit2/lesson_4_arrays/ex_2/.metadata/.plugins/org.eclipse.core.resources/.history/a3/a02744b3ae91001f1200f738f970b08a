#include <stdio.h>

int main(){

	int n=0;


	printf("enter the number of elements \n");
	fflush(stdout);
	scanf("%d",&n);


	int *arr = (int*) malloc ( n * sizeof(int) );

	for(int i=0 ; i<n ; i++)

	{
		printf("enter the %d number \n",i);
		fflush(stdout);
		scanf("%d",arr[i]);

	}










	return 0;
}

