#include <stdio.h>

int main(){
	int matrix_1[2][2];
	int matrix_2[2][2];
	int matrix_3[2][2];

	for(int i=0 ; i<2 ; i++)
	{
		for(int x=0 ; x<2 ; x++)
		{
			printf("enter the element [%d][%d] \n",i,x);
			fflush(stdout);
			scanf("%d",&matrix_1[i][x]);
		}

	}


	return 0;
}

