#include <stdio.h>

int main(){
	int x=0;
	int i;

	char str[100];
	printf("enter a string:");
	fflush(stdout);
	gets(str);


	for( i=0 ; i>0 ; i++){
		if( str[i] != '\0')
		x++;
		else
		break;
	}
	printf("length equals %d",i);
	return 0;
}

